{
  "name": "getter-robo",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "format": "prettier --write \"src/server/*.ts\" \"test/server/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "node dist/main.js start",
    "serve": "npm-run-all --parallel 'watch:*'",
    "build": "npm-run-all --parallel 'build:*'",
    "build:client": "parcel build src/Client/index.html",
    "build:server": "nest build",
    "watch:client": "parcel serve --hmr-port 9888 src/Client/index.html",
    "watch:server": "nest start --watch",
    "sim": "SIM=simple npm run serve",
    "test": "npm-run-all 'test:*'",
    "test:client": "elm-test",
    "test:server": "jest"
  },
  "dependencies": {
    "@nestjs/axios": "0.0.3",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^8.0.5",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/websockets": "^8.0.5",
    "@prisma/client": "^2.28.0",
    "@types/webtorrent": "^0.109.1",
    "css-reset-and-normalize": "^2.3.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "googleapis": "^84.0.0",
    "moment": "^2.29.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rss-parser": "^3.12.0",
    "rxjs": "^7.2.0",
    "socket.io": "^4.1.3",
    "swagger-ui-express": "^4.1.6",
    "webtorrent": "^1.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@parcel/transformer-elm": "^2.0.1",
    "@parcel/transformer-sass": "^2.0.1",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "autoprefixer": "^10.3.1",
    "elm": "^0.19.1-5",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "27.0.6",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.1",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.3.2",
    "prisma": "^2.28.0",
    "supertest": "^6.1.3",
    "tailwindcss": "^2.2.7",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
